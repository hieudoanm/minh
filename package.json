{
  "name": "chatbot",
  "version": "0.0.1",
  "description": "Chatbot",
  "main": "dist/index.js",
  "repository": "git@github.com:hieudoanm/chatbot.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "build:functions": "yarn workspace @chatbot/functions build",
    "build:visualiser": "yarn workspace @chatbot/visualiser build",
    "build:webhook": "yarn workspace @chatbot/webhook build",
    "dev:functions": "yarn workspace @chatbot/functions dev",
    "dev:visualiser": "yarn workspace @chatbot/visualiser dev",
    "dev:webhook": "yarn workspace @chatbot/webhook dev",
    "lint:functions": "yarn workspace @chatbot/functions lint",
    "lint:visualiser": "yarn workspace @chatbot/visualiser lint",
    "lint:webhook": "yarn workspace @chatbot/webhook lint",
    "start:functions": "yarn workspace @chatbot/functions start",
    "start:visualiser": "yarn workspace @chatbot/visualiser start",
    "start:webhook": "yarn workspace @chatbot/webhook start",
    "test:functions": "yarn workspace @chatbot/functions test",
    "test:visualiser": "yarn workspace @chatbot/visualiser test",
    "test:webhook": "yarn workspace @chatbot/webhook test"
  },
  "devDependencies": {
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3"
  },
  "resolutions": {
    "ejs": "3.1.8",
    "glob-parent": "6.0.2",
    "got": "12.5.2"
  },
  "workspaces": [
    "serverless/*",
    "web/*"
  ]
}
